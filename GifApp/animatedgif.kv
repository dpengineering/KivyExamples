#:import AnimatedGif animatedgif.AnimatedGif

<FloatLayout>:
    Button:
        text: "Hello"
        size_hint: 0.2, 0.1
        pos_hint: {"center_x": 0.3, "center_y": 0.5}

    AnimatedGif:
        source: 'GifApp/cat.gif'
        size_hint: 0.2, 0.2
        pos_hint: {"center_x": 0.7, "center_y": 0.5}
        on_press:
            app.gif_pressed()
